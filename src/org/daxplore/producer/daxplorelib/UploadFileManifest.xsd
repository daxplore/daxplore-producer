<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema">

	<xsd:element name="daxploreUploadFileManifest">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element name="dataVersion" type="xsd:nonNegativeInteger" />
				<xsd:element name="supportedLocales" type="supportedLocaleType" />
				<xsd:element name="defaultLocale" type="defaultLocaleType" />
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>

	<xsd:simpleType name="language-BCP47-Type">
		<xsd:restriction base="xsd:language" />
	</xsd:simpleType>

	<xsd:complexType name="supportedLocaleType">
		<xsd:sequence maxOccurs="unbounded">
			<xsd:element name="language-BCP47" type="language-BCP47-Type" />
		</xsd:sequence>
	</xsd:complexType>
	
	<xsd:complexType name="defaultLocaleType">
		<xsd:sequence>
			<xsd:element name="language-BCP47" type="language-BCP47-Type" />
		</xsd:sequence>
	</xsd:complexType>

</xsd:schema>
